{
  "index_patterns": [
    "configurator-index"
  ],
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0
    },
    "mappings": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "type": {
          "type": "keyword",
          "index": true
        },
        "code": {
          "type": "keyword",
          "index": true
        },
        "name": {
          "type": "keyword",
          "index": false
        },
        "price": {
          "type": "double",
          "index": false
        }
      }
    }
  }
}
